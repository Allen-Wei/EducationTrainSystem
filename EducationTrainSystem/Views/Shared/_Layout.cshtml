<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - Education</title>
    <link href="~/Styles/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Styles/font-awesome.min.css" rel="stylesheet" />

    <script src="~/Vendors/jquery-2.1.3.min.js" type="text/javascript"></script>
    <script src="~/Vendors/amplify.min.js"></script>
    <script src="~/Vendors/bootstrap.min.js"></script>

    <style type="text/css">
        * {
            font-family: 微软雅黑, sans-serif;
        }

        .edu-footer div.row {
            background-color: #f5f5f5;
            color: #0094ff;
            margin-top: 15px;
        }
    </style>

    @RenderSection("header", false)
</head>
<body>
    <div class="container-fluid edu-body">
        @RenderBody()
    </div>
    <div class="container-fluid edu-footer">
        <div class="row" style="background-color:#f5f5f5; color:#0094ff;">
            <div class="col-sm-12">
                @@2015 Alan
            </div>
        </div>
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>
